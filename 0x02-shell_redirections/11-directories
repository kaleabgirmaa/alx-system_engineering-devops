#!/bin/bash

# Initialize the count to 0
count=0

# Iterate over each entry in the current directory
for entry in $(ls -a)
do
    # Check if the entry is a directory and not the current or parent directory
    if [[ -d $entry && $entry != "." && $entry != ".." ]]
    then
        # Increment the count
        ((count++))
    fi
done

# Print the count
echo $count
